keywords = {Astrophysics,General Relativity and Quantum Cosmology,High Energy Physics - Phenomenology,High Energy Physics - Theory},
month = {jul},
number = {13},
pages = {3137--3169},
primaryClass = {hep-th},
title = {{FayetIliopoulos terms in supergravity and cosmology}},
